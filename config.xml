<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.designroom.cristi"
        versionCode = "20"
        version     = "2.0.1">


    <!-- versionCode is optional and Android only -->
    <preference name="DisallowOverscroll" value="true" />

    <!--
          id          = "com.designroom.cristi"             // ios
          id          = "cristi.flappy.example"             // android
     -->
    <!-- start plugin -->
    <!-- <gap:plugin name="cordova-plugin-admob" source="npm" /> -->


    <preference name="permissions" value="none"/>
    <preference name="android-build-tool" value="gradle" />
    <gap:plugin name="cordova-plugin-admobpro" source="npm" />
    <framework src="com.google.android.gms:play-services-ads:+" />

    <!-- end plugin -->



    <name>Flappy Cristi</name>

    <description>
        Flappy Cristi is an interactive game desired with love and fun.
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        DesignRoom
    </author>


<!-- START SPLASH -->
<!-- iPhone and iPod touch -->
<splash src="assets/splashscreen.png" platform="ios" width="320" height="480" />
<splash src="assets/splashscreen.png" platform="ios" width="640" height="960" />

<!-- iPhone 5 / iPod Touch (5th Generation) -->
<splash src="assets/splashscreen.png" platform="ios" width="640" height="1136" />

<!-- iPhone 6 -->
<splash src="assets/splashscreen.png" platform="ios" width="750" height="1334" />
<splash src="assets/splashscreen.png" platform="ios" width="1242" height="2208" />
<splash src="assets/splashscreen.png" platform="ios" width="2208" height="1242" />

<!-- iPad -->
<splash src="assets/splashscreen.png" platform="ios" width="768" height="1024" />
<splash src="assets/splashscreen.png" platform="ios" width="1024" height="768" />

<!-- Retina iPad -->
<splash src="assets/splashscreen.png" platform="ios" width="1536" height="2048" />
<splash src="assets/splashscreen.png" platform="ios" width="2048" height="1536" />


<!-- ANDROID -->
<splash src="assets/splashscreen.png" platform="android" qualifier="ldpi" />
<splash src="assets/splashscreen.png" platform="android" qualifier="mdpi" />
<splash src="assets/splashscreen.png" platform="android" qualifier="hdpi" />
<splash src="assets/splashscreen.png" platform="android" qualifier="xhdpi" />
<splash src="assets/splashscreen.png" platform="android" qualifier="fr-xhdpi" />
<splash src="assets/splashscreen.png" platform="android" qualifier="port-xxhdpi" />
<splash src="assets/splashscreen.png" platform="android" qualifier="land-xxhdpi" />

<splash src="assets/splashscreen.png" platform="winphone" />

<splash platform="winphone" width="1152" height="1920" src="assets/splashscreen.png" />
<!-- END SPLASH -->

    <!-- ANDROID ICONS -->
    <icon src="assets/icons/android/mipmap-ldpi/ic_launcher.png" platform="android" qualifier="ldpi" />
    <icon src="assets/icons/android/mipmap-mdpi/ic_launcher.png" platform="android" qualifier="mdpi" />
    <icon src="assets/icons/android/mipmap-hdpi/ic_launcher.png" platform="android" qualifier="hdpi" />
    <icon src="assets/icons/android/mipmap-xhdpi/ic_launcher.png" platform="android" qualifier="xhdpi" />
    <icon src="assets/icons/android/mipmap-xxhdpi/ic_launcher.png" platform="android" qualifier="xxhdpi" />
    <icon src="assets/icons/android/mipmap-xxxhdpi/ic_launcher.png" platform="android" qualifier="fr-xxhdpi" />

    <!-- iPhone 6 / 6+ -->
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="180" height="180" />

    <!-- iPhone / iPod Touch  -->
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="60" height="60" />
    <icon src="assets/icons/ios/Icon-72.png" platform="ios" width="72" height="72" />


    <icon src="assets/icons/ios/Icon-60@2x.png" platform="ios" width="120" height="120" />

    <!-- iPad -->
    <icon src="assets/icons/ios/Icon-76.png" platform="ios" width="76" height="76" />
    <icon src="assets/icons/ios/Icon-76@2x.png" platform="ios" width="152" height="152" />

    <!-- Settings Icon -->
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="29" height="29" />
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="58" height="58" />

    <!-- Spotlight Icon -->
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="40" height="40" />
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="80" height="80" />

    <!-- iPhone / iPod Touch -->
    <icon src="assets/icons/ios/icon.png" platform="ios" width="57" height="57" />
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="114" height="114" />

    <!-- iPad -->
    <icon src="assets/icons/ios/Icon-72.png" platform="ios" width="72" height="72" />
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="144" height="144" />

    <!-- iPhone Spotlight and Settings Icon -->
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="29" height="29" />
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="58" height="58" />

    <!-- iPad Spotlight and Settings Icon -->
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="50" height="50" />
    <icon src="assets/icons/ios/iTunesArtwork.png" platform="ios" width="100" height="100" />
</widget>
